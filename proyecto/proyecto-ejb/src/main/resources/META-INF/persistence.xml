<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
	<persistence-unit name="proyecto-ejb"
		transaction-type="JTA">
		<jta-data-source>jdbc/sii</jta-data-source>
		<class>es.uma.proyecto.Autorizacion</class>
		<class>es.uma.proyecto.Persona_Autorizada</class>
		<class>es.uma.proyecto.Cliente</class>
		<class>es.uma.proyecto.Empresa</class>
		<class>es.uma.proyecto.Cuenta_Fintech</class>
		<class>es.uma.proyecto.Cuenta</class>
		<class>es.uma.proyecto.Cuenta_Referencia</class>
		<class>es.uma.proyecto.Depositado_en</class>
		<class>es.uma.proyecto.Divisa</class>
		<class>es.uma.proyecto.Individual</class>
		<class>es.uma.proyecto.Pooled_Account</class>
		<class>es.uma.proyecto.Segregada</class>
		<class>es.uma.proyecto.Transaccion</class>
		<class>es.uma.proyecto.Autorizacion_PK</class>
		<class>es.uma.proyecto.Depositado_en_PK</class>
		<class>es.uma.proyecto.Usuario</class>
		<shared-cache-mode>NONE</shared-cache-mode>
		<properties>
			<property
				name="javax.persistence.schema-generation.database.action"
				value="create" />
		</properties>
	</persistence-unit>
</persistence>
